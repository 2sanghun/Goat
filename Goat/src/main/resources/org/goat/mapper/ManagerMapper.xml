<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.goat.mapper.ManagerMapper">
	<select id="memberlist" resultType="org.goat.model.MemberVO">
		select * from member;
	</select>

	<select id="memdetail" resultType="org.goat.model.MemberVO">
		select * from member where id=#{id} ;
	</select>

	<!-- <update id="modify"> update board set tutle='', content='' where bno=14; 
		</update> -->

	<delete id="remove">
		delete from member where id=#{id};
	</delete>

</mapper>

